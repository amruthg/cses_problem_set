#include<bits/stdc++.h>
using namespace std;

//what is the number of trailing zeores?

//the power of a prime in a n! is given by:-

// = n//p + n//p^2 +n//p^3.....

void expo(long n){

	long ans=0;
	while(n){
		n=n/5;
		ans+=n;
	}
	cout<<ans;
}

int main(){

	long n;
	cin>>n;

	expo(n);
	return 0;
}
