#include<bits/stdc++.h>
using namespace std;

int main(){
    int n;
    cin>>n;

    if(n>1 and n<4){
        cout<<"NO SOLUTION";
        return 0;
    }
    else{
        for(int i=2;i<=n;i+=2){
            cout<<i<<" ";
        }
        for(int i=1;i<=n;i+=2){
            cout<<i<<" ";
        }
        return 0;
    }
}

//mistake 1: not able to realise the simple logic

//mistake 2: not being able to see that for n=1, the question is satisfied.

//mistake 3:not able to see that exchanging the for loops can solve the issue of the case n==4.
